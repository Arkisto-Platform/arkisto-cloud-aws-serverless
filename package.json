{
  "name": "arkisto-cloud-aws",
  "version": "0.1.0",
  "bin": {
    "arkisto-cloud-aws": "bin/arkisto-cloud-aws.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "node_modules/.bin/cdk synth",
    "cdk:deploy": "node_modules/.bin/cdk deploy --require-approval any-change",
    "cdk:destroy": "node_modules/.bin/cdk destroy --require-approval any-change",
    "cdk:diff": "./node_modules/.bin/cdk diff",
    "start:service": "source cdk_service.sh start",
    "stop:service": "source cdk_service.sh stop"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.62.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "^1.76.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.76.0",
    "@aws-cdk/aws-ecs": "^1.76.0",
    "@aws-cdk/aws-ecs-patterns": "^1.76.0",
    "@aws-cdk/aws-efs": "^1.76.0",
    "@aws-cdk/core": "1.76.0",
    "source-map-support": "^0.5.16"
  }
}
