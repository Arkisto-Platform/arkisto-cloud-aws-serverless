{
  "app": "npx ts-node bin/arkisto-cloud-aws.ts",
  "context": {
    "cloud_stack_id": "SeafoodSafety",
    "availability_zones": [
      "ap-southeast-2a",
      "ap-southeast-2b",
      "ap-southeast-2c"
    ],
    "base": {
      "ocfl": "/Users/moises/seafood/oni-express/ocfl",
      "config": "/Users/moises/seafood/oni-express/config",
      "description": "SeafoodSafety ONI site",
      "volume_name": "oni-express-volume",
      "service": {
        "memory": 3072,
        "cpu": 512
      },
      "load_balancer": {
        "cpu": 512,
        "memory": 512
      },
      "express": {
        "location": "/Users/moises/seafood/oni-express",
        "memory": 1024,
        "cpu": 1
      },
      "indexer": {
        "location": "/Users/moises/seafood/oni-indexer",
        "memory": 1024,
        "cpu": 1
      },
      "solr": {
        "location": "docker/solr",
        "memory": 2048,
        "cpu": 2
      },
      "memcached": {
        "memory": 512,
        "cpu": 1
      },
      "data_service": {
        "memory": 1024,
        "cpu": 256
      },
      "ssh": {
        "location": "docker/ssh",
        "public_key": "key/id.rsa.pub",
        "memory": 256,
        "cpu": 1
      }
    },
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true"
  }
}
